{
    "env": {
        "production": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": ["last 2 version", "ie 9", "safari >= 8"]
                    },
                    "modules": false,
                    "debug": false
                }]
            ],
            "plugins": [
                "transform-runtime",
                "transform-object-rest-spread",
                "babel-plugin-syntax-dynamic-import"
            ]
        },
        "development": {
            "presets": [
                ["env", {
                    "targets": {
                        "chrome": 52
                    },
                    "modules": false,
                    "debug": true
                }]
            ],
            "plugins": [
                "transform-object-rest-spread",
                "babel-plugin-syntax-dynamic-import"
            ]
        }
    }
}
